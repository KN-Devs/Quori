{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <div class="block">
        <h2 class="pb-30">Inscription</h2>
        {{ form_start(signupForm) }}
            {{ form_rest(signupForm) }}
            <button type="submit" class="btn btn-primary mr-10">S'inscrire</button>
            <small>
                <a href="{{ path('signin') }}">Déjà inscrit ?</a>
            </small>
        {{ form_end(signupForm) }}
    </div>
{% endblock %}
